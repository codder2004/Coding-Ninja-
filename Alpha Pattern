from math import *
from collections import *
from sys import *
from os import *

## Read input as specified in the question.
## Print output as specified in the question.
n=int(input())
i=0
while(i<n):
    j=0
    while ( j <= i):
        u = chr( ord('A')+i)
        print(u,end='')
        j=j+1
    i=i+1
    print()
